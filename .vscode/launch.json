    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Single test",
            "program": "${workspaceFolder}/test/${input:tests}",
            "outputCapture": "std"
        },
      	{
            "name": "Unit tests",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/tap/bin/run.js",
            "stopOnEntry": false,
            "args": [
                "-J",
                "-T",
                "--node-arg=--preserve-symlinks",
                "test"
            ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "UT_ENV": "test",
                "NODE_PATH": "${workspaceRoot}/dev"
            },
            "sourceMaps": false,
            "outputCapture": "std",
            "linux": {
                "protocol": "inspector"
            }
        }
    ],
